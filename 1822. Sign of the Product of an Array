/**
 * @param {number[]} nums
 * @return {number}
 */
var arraySign = function(nums) {
    let n = nums.length;
    let product =1;
    for(let i =0; i<n;i++){
        product*=nums[i];
    }
    if(product>0){
        return "1";
    }
    else if(product<0){
        return "-1";
    }
    else
    return "0";
    
};
